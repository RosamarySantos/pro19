    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".vista.FragmentStore">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/ed_user_store"
